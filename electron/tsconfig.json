{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "es2019",
    "outDir": "../build",
    "rootDir": ".",
    "noEmit": false,
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "jsx": "react"
  }
}
